{
  "name": "react-immutable",
  "version": "1.0.0",
  "description": "Higher order component for managing hierarchical, immutable app state",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "test": "babel-node node_modules/.bin/_mocha --compilers js:babel-register -u bdd $(find ./test -type d -name unit)",
    "coverage": "babel-node node_modules/.bin/isparta cover --include-all-sources --root ./src --report text node_modules/.bin/_mocha -- -u bdd $(find ./test -type d -name unit)",
    "compile": "mkdir -p ./dist && babel src --out-dir dist",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steveorsomethin/react-immutable.git"
  },
  "keywords": [
    "react",
    "immutable"
  ],
  "author": {
    "name": "Netflix"
  },
  "licenses": [
    {
      "type": "Apache License, Version 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "bugs": {
    "url": "https://github.com/steveorsomethin/react-immutable/issues"
  },
  "publishConfig": {
    "registry": "http://artifacts.netflix.com/api/npm/npm-local"
  },
  "homepage": "https://github.com/steveorsomethin/react-immutable#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "babel-runtime": "^5.5.6",
    "chai": "^2.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "isparta": "^4.0.0",
    "istanbul": "^0.3.15",
    "jsdom": "3.x.x",
    "mocha": "^2.2.5",
    "react": "^0.13.1",
    "sinon": "^1.15.3"
  }
}
